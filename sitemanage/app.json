{
  "expo": {
    "name": "sitemanage",
    "slug": "sitemanage",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/site-manager-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hintlord.sitemanage"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sitemanage-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hintlord.sitemanage"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/sitemanage-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "824143ba-ad01-4e22-b4e3-6a4015dfc4f1"
      }
    }
  }
}
